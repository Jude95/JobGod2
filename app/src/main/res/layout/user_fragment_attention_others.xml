<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:recycler="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.ant.jobgod.jobgod.module.user.AttentionOthersFragment">

    <android.support.v7.widget.RecyclerView android:id="@+id/recycler"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        recycler:layout_empty="@layout/view_empty"
        recycler:layout_moreProgress="@layout/view_progress"
        recycler:layout_progress="@layout/view_progress"
        recycler:scrollbarStyle="insideInset" />

</RelativeLayout>
